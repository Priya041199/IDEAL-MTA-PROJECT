<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog id="settingsDialog" confirm="onConfirmViewSettingsDialog" cancel="onCancelFilter" class="sapUiSizeCompact">
		<filterItems>
			<ViewSettingsFilterItem text="Country" items="{countrySet>/}" key="1">
				<items>
					<ViewSettingsItem text="{countrySet>LANDX}" key="{countrySet>LAND1}"/>
				</items>
			</ViewSettingsFilterItem>
			
			<ViewSettingsCustomItem text="Creation Date" key="3">
                <customControl>
                <FlexBox justifyContent="Center">
                   <DateRangeSelection id="date_fId" placeholder="Select Date Range"/>
                   </FlexBox>
                </customControl>
            </ViewSettingsCustomItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>