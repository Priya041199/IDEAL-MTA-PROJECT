<core:FragmentDefinition id="frag" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:fb="sap.ui.comp.filterbar">
	<Page showHeader="false" class="sapUiSizeCompact">
		<!-- <Button id = "exportTable1" icon="sap-icon://excel-attachment" type="Emphasized" press="onPress" ariaLabelledBy="editButtonLabel"/> -->
		<fb:FilterBar search="onSubmit" clear="onClear" showClearOnFB="true" showClearButton="true" showGoOnFB="true" filterBarExpanded="true"
			filterChange="false" deltaVariantMode="false" showRestoreButton="false" showFilterConfiguration="false">
			<fb:filterGroupItems>
				<fb:FilterGroupItem groupName="G1" name="A" label="Start Date" partOfCurrentVariant="true" visibleInFilterBar="true">
					<fb:control>
						<DatePicker id="d1" width="15%" maxDate="{viewModel>/maxDate}" placeholder="From Date"  valueFormat="yyyy-MM-ddTHH:mm:ss" change="onSelectionChange"/>
					</fb:control>
				</fb:FilterGroupItem>
				<fb:FilterGroupItem groupName="G1" name="B" label="End Date" partOfCurrentVariant="true" visibleInFilterBar="true">
					<fb:control>
						<DatePicker id="d2" width="15%" placeholder="To Date" maxDate="{viewModel>/maxDate}" valueFormat="yyyy-MM-ddTHH:mm:ss" change="onSelectionChange"/>
					</fb:control>
				</fb:FilterGroupItem>

				<fb:FilterGroupItem groupName="G1" name="C" label="Order Number" partOfCurrentVariant="true" visibleInFilterBar="true">
					<fb:control>
						<Input id="OrderNumberDialog" width="95%" showSuggestion="true" showValueHelp="true" valueHelpOnly="true" valueHelpRequest="handleValueHelp"/>
					</fb:control>
				</fb:FilterGroupItem>
				
				<fb:FilterGroupItem groupName="G1" name="E" label="Division" partOfCurrentVariant="true" visibleInFilterBar="true">
					<fb:control>
						<Input id="DivisionDialog" width="95%" showSuggestion="true" showValueHelp="true" valueHelpOnly="true" valueHelpRequest="handleValueHelp"/>
					</fb:control>
				</fb:FilterGroupItem>

				<fb:FilterGroupItem groupName="G1" name="D" label="Material" partOfCurrentVariant="true" visibleInFilterBar="true">
					<fb:control>
						<Input id="MaterialDialog" showValueHelp="true" valueHelpOnly="true" valueHelpRequest="handleValueHelp" width="95%"/>
					</fb:control>
				</fb:FilterGroupItem>

				<fb:FilterGroupItem groupName="G1" id="selectBox" visible="false" name="F" label="Status" partOfCurrentVariant="true"
					visibleInFilterBar="true">
					<fb:control>
						<!--<Select forceSelection="false" id="idSelect">-->
						<!--	<core:Item key="Blocked" text="Blocked"/>-->
						<!--	<core:Item key="Cancelled" text="Cancelled"/>-->
						<!--	<core:Item key="Credit-Check Blocked" text="Credit-Check Blocked"/>-->
						<!--	<core:Item key="Released" text="Released"/>-->
						<!--	<core:Item key="Stock Allocated" text="Stock Allocated"/>-->
						<!--	<core:Item key="Partially Allocated" text="Partially Allocated"/>-->
						<!--	<core:Item key="Complelety Invoiced" text="Complelety Invoiced"/>-->
						<!--	<core:Item key="Partially Invoiced" text="Partially Invoiced"/>-->
						<!--	<core:Item key="Dispatched" text="Dispatched"/>-->
						<!--	<core:Item key="Not Dispatched" text="Not Dispatched"/>-->
						<!--</Select>-->
						<MultiComboBox id="multicombobox" selectionChange="handleSelectionChange" selectionFinish="handleSelectionFinish">
							<core:Item key="Blocked" text="Blocked"/>
							<core:Item key="Cancelled" text="Cancelled"/>
							<core:Item key="Credit-Check Blocked" text="Credit-Check Blocked"/>
							<core:Item key="Released" text="Released"/>
							<core:Item key="Stock Allocated" text="Stock Allocated"/>
							<core:Item key="Partially Allocated" text="Partially Allocated"/>
							<core:Item key="Complelety Invoiced" text="Complelety Invoiced"/>
							<core:Item key="Partially Invoiced" text="Partially Invoiced"/>
							<core:Item key="Dispatched" text="Dispatched"/>
							<core:Item key="Not Dispatched" text="Not Dispatched"/>
						</MultiComboBox>
					</fb:control>
				</fb:FilterGroupItem>
			</fb:filterGroupItems>
		</fb:FilterBar>
		<!--<ScrollContainer id="scrollcontainer" visible="true" height="420px" width="100%" vertical="true" horizontal="true" focusable="false">-->
			<Table id="idViewPO" mode="SingleSelectMaster" selectionChange="ShowOrderDetails" visible="true" items="{orderDetail>/results}"
				growing="true" sticky="HeaderToolbar,ColumnHeaders">

				<headerToolbar>
					<OverflowToolbar>
								<!-- <SearchField id="onSearchMasterData" width="30%" showRefreshButton="false" placeholder="Enter Claim Request Number" liveChange="onSearch" class="sapUiSmallMargin"/> -->
								<!-- <content> -->
									<ToolbarSpacer/>
									<Button id = "exportTable" icon="sap-icon://excel-attachment" type="Emphasized" press="onPress" ariaLabelledBy="editButtonLabel"/>
								<!-- </content> -->
							</OverflowToolbar>
				</headerToolbar>

				<columns>
					<Column demandPopin="true">
						<Label text="Order Number" design="Bold"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="Order Line" design="Bold"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="Creation Date" design="Bold"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="Division" design="Bold"/>
					</Column>
					<Column demandPopin="true">
						<Label text="Material" design="Bold"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="Order Quantity" design="Bold"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="Total Value" design="Bold"/>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Label text="Status" design="Bold"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text text="{orderDetail>OrderNo}"/>
							<Text text="{orderDetail>OrderLine}"/>
							<!--<Text text="{orderDetail>Date}"/>-->
							<Text text="{path: 'orderDetail>Date', formatter : '.formatter.formatDate'}"/>
							<Text text="{orderDetail>Vtext}"/>
							<Text text="{orderDetail>MaterialDesc}"/>
							<Text text="{orderDetail>OrderQuantity}"/>
							<Text text="{orderDetail>TotalValue}"/>
							<Text text="{orderDetail>Status}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		<!--</ScrollContainer>-->
	</Page>
	<!--</f:SimpleForm>-->
</core:FragmentDefinition>