<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog id="settingsDialog" confirm="onConfirmViewSettingsDialog" cancel="onCancelFilter" class="sapUiSizeCompact">
		<filterItems>
			<ViewSettingsFilterItem text="RGA Request Number" items="{grNoModel>/value}" key="request">
				<items>
					<ViewSettingsItem text="{grNoModel>RGA_NO}" key="request"/>
				</items>
			</ViewSettingsFilterItem>
			
			<ViewSettingsCustomItem text="Creation Date" key="3">
                <customControl>
                <FlexBox justifyContent="Center">
                   <DateRangeSelection id="date_fId" placeholder="Select Date Range" maxDate="{minDateModel>/minDate}"/>
                   </FlexBox>
                </customControl>
            </ViewSettingsCustomItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>