<core:FragmentDefinition xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:calendar="sap.ui.unified">
	<Dialog title="Select Details" afterClose="onSelectDialogClose" class="sapUiSizeCompact" beforeClose="onBeforeClose">
		<content>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12"
				adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
				singleContainerFullSize="false">
				<!--<Label text="Transaction Period"/>-->
				<VBox>
					<Label text="From Date:"/>
					<DatePicker id="fromdate" class="disablekb" valueFormat="yyyy-MM-ddTHH:mm:ss" visible="true" required="true"/>
					<Label text="To Date:"/>
					<DatePicker id="dateToId" class="disablekb" change="onChange" valueFormat="yyyy-MM-ddTHH:mm:ss" visible="true" required="true"/>
				</VBox>
			</f:SimpleForm>
		</content>
		<!--<beginButton>-->
		<!--	<Button text="Submit" press="onSubmit"></Button>-->
		<!--</beginButton>-->
		<!--<endButton>-->
		<!--	<Button text="Cancel" press="onCancel"></Button>-->
		<!--</endButton>-->
	</Dialog>
</core:FragmentDefinition>