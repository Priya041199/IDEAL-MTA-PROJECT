<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog id="idMasterEditDialog" title="" contentWidth="400px" titleAlignment="Center" class="sapUiSizeCompact">
		<f:SimpleForm id="idSimpleFormEditMaster" editable="false" layout="ColumnLayout" title="" columnsM="2" columnsL="2" columnsXL="2">
			<f:content></f:content>
		</f:SimpleForm>
		<buttons>
			<Button text="Submit" press="onSaveMasterEdit" visible="{oPropertyModel>/submit}" icon="sap-icon://accept"/>
			<Button text="Close" press="onCloseMasterEdit" icon="sap-icon://cancel"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>