<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Select Country" contentHeight="80%"
		class="sapUiSizeCompact">
		<subHeader>
			<Toolbar>
				<SearchField id="bcSrchId" liveChange="handleBankCountrySearch" />
			</Toolbar>
		</subHeader>

		<List id="bankCountry_Id" items="{bankCountryJson>/results}"
			mode="SingleSelectMaster" selectionChange="handleOtherBankCountrySelection">
			<StandardListItem title="{bankCountryJson>Landx}" description="{bankCountryJson>Land1}"/>
		</List>
		
		<buttons>
			<Button text="Cancel" press="closeOtherBankCountryDialog" type="Emphasized" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>