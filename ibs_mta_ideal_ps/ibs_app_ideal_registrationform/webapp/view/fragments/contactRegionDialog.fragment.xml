<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Select Region" contentHeight="80%"
		class="sapUiSizeCompact">
		<subHeader>
			<Toolbar>
				<SearchField id="con_regionSrchId" liveChange="handleOTHContactRegionSearch" />
			</Toolbar>
		</subHeader>

		<List id="contactreg_listId" items="{RegionJson1>/results}"
			mode="SingleSelectMaster" selectionChange="handleOTHContactRARegionChange">
			<StandardListItem title="{RegionJson1>Bezei}" description="{RegionJson1>Bland}"/>
		</List>

		<buttons>
			<Button text="Cancel" press="closeOTHContactRegionDialog" type="Emphasized" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>