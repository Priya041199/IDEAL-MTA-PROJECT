<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog class="sapUiSizeCompact" id="viewSettingsDialog" cancel="handleCancel"
	confirm="onConfirmViewSettingsDialog">
		<filterItems>
			<ViewSettingsFilterItem text="Claim Request Number" key="request" items="{crNoModel>/value}">
				<items>
					<ViewSettingsItem text="{crNoModel>CR_NO}" key="request"/>
				</items>
			</ViewSettingsFilterItem>
			
			 <ViewSettingsCustomItem text="Creation Date" key="3">
                <customControl>
                <FlexBox justifyContent="Center">
                   <DateRangeSelection id="date_fId" placeholder="Select Date Range" maxDate="{minDateModel>/minDate}"/>
                   </FlexBox>
                </customControl>
            </ViewSettingsCustomItem>
			
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>