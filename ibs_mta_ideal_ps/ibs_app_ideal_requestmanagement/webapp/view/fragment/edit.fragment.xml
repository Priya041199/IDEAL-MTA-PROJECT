<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:ui="sap.ui.table" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" width="100px" xmlns:layout="sap.ui.layout">
	<Dialog title="{changeRequestModel>/REQUEST_NO} -  {changeRequestModel>/DIST_NAME1}" contentWidth="400px" draggable="true" resizable="true" class="sapUiSizeCompact" titleAlignment="Center" contentHeight="auto">
		
		<IconTabBar
		id="idIconTabBarNoIcons"
		expanded="true"
		expandable="false"
		class="sapUiResponsiveContentPadding" select="onIconTab" selectedKey="{oPropertyModel>/selectedTabKey}">
		<items>
			<IconTabFilter text="{i18n>tabFilter1}" key="RequestChangeKey" id="idRequestChange" visible="{oPropertyModel>/RequestTypeTab}">
				<!--<Text text="Attachments go here ..." />-->
					<f:SimpleForm id="SimpleFormChange354" editable="true" layout="ColumnLayout" title=""
				singleContainerFullSize="false" visible="true">
				<f:content>

					<Label text="{i18n>requestTypelabel1}" design="Bold" id="requestTypelabel1_id2"/>
