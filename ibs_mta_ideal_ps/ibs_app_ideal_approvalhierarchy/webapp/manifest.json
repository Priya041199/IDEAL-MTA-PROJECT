{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.ibs.ibsappidealapprovalhierarchy",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.14.2",
      "toolsId": "84f2373f-602c-46a8-a5ae-f643508eb6c0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/ideal-fiori-approval-hierarchy/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "approvalhierarchy-display": {
          "semanticObject": "approvalhierarchy",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.126.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ibs.ibsappidealapprovalhierarchy.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {
        "flexibleColumnLayout": {
          "defaultTwoColumnLayoutType": "TwoColumnsBeginExpanded",
          "defaultThreeColumnLayoutType": "ThreeColumnsMidExpanded"
      },
      "routerClass": "sap.f.routing.Router"
      },
      "routes": [
        {
          "pattern": ":?query:",
          "name": "MasterEntityAndTypeList",
          "target": ["MasterEntityAndTypeList"]
        },
        {
          "pattern": "MasterEntityAndType({key}):?query:",
          "name": "MasterEntityAndTypeObjectPage",
          "target": [
            "MasterEntityAndTypeList",
            "MasterEntityAndTypeObjectPage"
        ]
        }
      ],
      "targets": {
        "MasterEntityAndTypeList": {
          "type": "Component",
          "id": "MasterEntityAndTypeList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/MasterEntityAndType",
              "variantManagement": "Page",
              "navigation": {
                "MasterEntityAndType": {
                  "detail": {
                    "route": "MasterEntityAndTypeObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "selectionMode": "None"
                  },
                  "columns": {
                    "DataField::TYPE": {
                      "width": "550px"
                    },
                    "DataField::ENTITY_CODE": {
                      "width": "670px"
                    }
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          },
          "controlAggregation": "beginColumnPages",
          "contextPattern": ""
        },
        "MasterEntityAndTypeObjectPage": {
          "type": "Component",
          "id": "MasterEntityAndTypeObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/MasterEntityAndType",
              "controlConfiguration": {
                "TO_MASTER_APPROVAL_HIERARCHY/@com.sap.vocabularies.UI.v1.LineItem#ApprovalHierarchy": {
                    "tableSettings": {
                        "creationMode": {
                            "createAtEnd": true
                        }
                    },
                    "columns": {
                        "DataField::ACCESS_APPROVE": {
                            "width": "105px"
                        },
                        "DataField::ACCESS_REJECT": {
                            "width": "105px"
                        },
                        "DataField::ACCESS_EDIT": {
                            "width": "105px"
                        },
                        "DataField::ACCESS_SENDBACK": {
                            "width": "105px"
                        },
                        "DataField::ROLE_CODE": {
                            "width": "270px"
                        },
                        "DataField::ACCESS_HOLD": {
                            "width": "105px"
                        }
                    }
                }
            }
            }
          },
          "controlAggregation": "midColumnPages",
          "contextPattern": "/MasterEntityAndType({key})"
        }
      }
    },
    "rootView": {
      "viewName": "sap.fe.templates.RootContainer.view.Fcl",
      "type": "XML",
      "async": true,
      "id": "appRootView"
  }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "ibs_bs_ideal_ps"
  }
}
